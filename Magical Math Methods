    public void roundUp(double a) {
        a = a+0.5;
        int rounded = (int)Math.round(a);
        lblResultUp.setText(String.valueOf(rounded));
    }
    public void roundDown(double a) {
        a = a - 0.5;
        int rounded = (int)Math.round(a);
        lblResultDown.setText(String.valueOf(rounded));
    }
    
    public void findHypot(double a, double b) {
        double hypot =Math.sqrt((Math.pow(a,2) + Math.pow(b, 2)));
        hypot = (Math.round(hypot * 100)) / 100;
        lblResultHypot.setText(String.valueOf(hypot));
    }
    
    public void toRadians(double a) {
        double radians = a * 3.14 / 180;
        lblResultRadians.setText("" + radians);
    }
    
    public void reset() {
        lblResultLargest.setText("...");
        lblResultDown.setText("...");
        lblResultSmallest.setText("...");
        lblResultPower.setText("...");
        lblResultUp.setText("...");
        lblResultRadians.setText("...");
        lblResultHypot.setText("...");
        lblResultSqrt.setText("...");
    }
    public void clear() {
        tfLargest1.setText("");
        tfLargest2.setText("");
        tfSmallest1.setText("");
        tfSmallest2.setText("");
        tfBase.setText("");
        tfPower.setText("");
        tfSide1.setText("");
        tfSide2.setText("");
        tfDown.setText("");
        tfUp.setText("");
        tfSqrt.setText("");
        tfRadians.setText("");
    }
    
    private void btnLargestActionPerformed(java.awt.event.ActionEvent evt) {                                           
        reset();
        double num1 = Double.parseDouble(tfLargest1.getText());
        double num2 = Double.parseDouble(tfLargest2.getText());
        lblResultLargest.setText(String.valueOf(Math.max(num1,num2)));
    }                                          

    private void btnSmallestActionPerformed(java.awt.event.ActionEvent evt) {                                            
        reset();
        double num1 = Double.parseDouble(tfSmallest1.getText());
        double num2 = Double.parseDouble(tfSmallest2.getText());
        lblResultSmallest.setText(String.valueOf(Math.min(num1,num2)));
    }                                           

    private void btnRoundedDownActionPerformed(java.awt.event.ActionEvent evt) {                                               
        reset();
        double num = Double.parseDouble(tfDown.getText());
        roundDown(num);
    }                                              

    private void btnRoundedUpActionPerformed(java.awt.event.ActionEvent evt) {                                             
        reset();
        double num = Double.parseDouble(tfUp.getText());
        roundUp(num);
    }                                            

    private void btnSqrtActionPerformed(java.awt.event.ActionEvent evt) {                                        
        reset();
        double num = Double.parseDouble(tfSqrt.getText());
        lblResultSqrt.setText(String.valueOf(Math.sqrt(num)));
        
    }                                       

    private void btnPowerActionPerformed(java.awt.event.ActionEvent evt) {                                         
        reset();
        double base = Double.parseDouble(tfBase.getText());
        double power = Double.parseDouble(tfPower.getText());
        lblResultPower.setText(String.valueOf(Math.pow(base, power)));
    }                                        

    private void btnHypotActionPerformed(java.awt.event.ActionEvent evt) {                                         
        reset();
        double side1 = Double.parseDouble(tfSide1.getText());
        double side2 = Double.parseDouble(tfSide2.getText());
        lblResultHypot.setText(String.valueOf(Math.sqrt(Math.pow(side1,2) + Math.pow(side2,2))));
    }                                        

    private void btnRadiansActionPerformed(java.awt.event.ActionEvent evt) {                                           
        reset();
        double angle = Double.parseDouble(tfRadians.getText());
        lblResultRadians.setText(String.valueOf(angle * 3.14 / 180));
    }                                          

